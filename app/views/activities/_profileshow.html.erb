
<div class="profile-show">

<!--   <% if @user.nil? && @user == current_user %>
  <% end %> -->

  <% if !current_user.photo.blank? %>
    <div class="user-profile"><%= cl_image_tag current_user.photo%></div>
  <% else %>
    <img src="<%= current_user.facebook_picture_url %> " alt="" >
  <% end %>
<div class="profile-div">
  <ul style="text-align:center;" >
    <li class="profile-info"><strong><%= current_user.name %></strong></li>
    <li ><%= current_user.gender %></li>
    <li><%= current_user.age %></li>
  </ul>
</div>
  <div class="buttons m-4">
    <%= link_to "Edit profile", edit_user_registration_path, class: "btn btn-outline-secondary edit-profile-btn" %>

    <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "btn btn-outline-secondary log-out-btn" %>

  </div>
</div>
